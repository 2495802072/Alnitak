config_version=5

[application]

config/name="Alnitak"
config/tags=PackedStringArray("alnitak")
run/main_scene="res://Game/scene/game.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Alnitak"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

G="*res://Global/scene/global.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/custom_nodes/plugin.cfg", "res://addons/fast_ui_plug/plugin.cfg", "res://addons/project_plug/plugin.cfg")

